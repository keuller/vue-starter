{
  "name": "vue-starter",
  "description": "Simple Vue Starter Project",
  "version": "1.0.0",
  "author": "Keuller Magalhaes <keuller.magalhaes@gmail.com>",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --inline --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules"
  },
  "license": "MIT",
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "modules": false
        }
      ]
    ],
    "plugins": [
      "transform-vue-jsx"
    ]
  },
  "dependencies": {
    "vue": "^2.1.10",
    "vue-router": "^2.2.0",
    "vuelm": "0.5.0"
  },
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-helper-vue-jsx-merge-props": "^2.0.2",
    "babel-loader": "^6.2.10",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.3.0",
    "babel-preset-es2015": "^6.22.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.0",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "file-loader": "^0.10.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "vue-loader": "^10.1.2",
    "vue-template-compiler": "^2.1.10",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.2.0"
  }
}
