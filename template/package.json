{
  "name": "vue-starter",
  "description": "Simple Vue Starter Project",
  "version": "1.1.0",
  "author": "Keuller Magalhaes <keuller.magalhaes@gmail.com>",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --inline --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules"
  },
  "license": "MIT",
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "modules": false
        }
      ]
    ],
    "plugins": [
      "transform-vue-jsx"
    ]
  },
  "dependencies": {
    "vue": "^2.2.4",
    "vue-router": "^2.3.0",
    "vuelm": "0.5.0"
  },
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-helper-vue-jsx-merge-props": "^2.0.2",
    "babel-loader": "^6.4.1",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.4.2",
    "babel-preset-es2015": "^6.24.0",
    "cross-env": "^3.2.4",
    "css-loader": "^0.27.3",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "^0.10.1",
    "less": "^3.0.0",
    "less-loader": "^2.2.3",
    "vue-loader": "^11.1.4",
    "vue-template-compiler": "^2.2.4",
    "webpack": "^2.2.1",
    "webpack-dev-server": "2.4.2"
  }
}
