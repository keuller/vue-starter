{
  "name": "vue-starter",
  "description": "Vue-Starter project",
  "version": "1.0.0",
  "author": "Keuller Magalhaes <keuller.magalhaes@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules"
  },
  "dependencies": {
    "vue": "^2.5.3",
    "vue-router": "^3.0.1",
    "vuelm": "^0.8.3"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.5.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "clean-webpack-plugin": "^0.1.17",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "node-sass": "^4.6.1",
    "sass-loader": "^6.0.6",
    "vue-loader": "^13.5.0",
    "vue-template-compiler": "^2.5.3",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  },
  "babel": {
    "presets": [
      "stage-2",
      [
        "env",
        {
          "modules": false
        }
      ]
    ],
    "plugins": [
      "transform-vue-jsx"
    ]
  },
  "engines": {
    "node": ">= 8.1.0",
    "npm": ">= 5.4.0"
  }
}
